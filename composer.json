{
    "name": "huangdijia/laravel-recaptcha-v3",
    "description": "recaptcha-v3 for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "recaptcha-v3"
    ],
    "homepage": "https://github.com/huangdijia/laravel-recaptcha-v3",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=8.0",
        "google/recaptcha": "^1.2",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Huangdijia\\Recaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Recaptcha\\RecaptchaServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src"
    }
}